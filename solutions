<script>
     $(document).ready(function() {
        $('#newsletter-submit').click(signUpForNewsletter);
    });

    function signUpForNewsletter() {

          var e = $('#newsletter-email');
          // var n = $('#newsletter-name');
          var ev = e.val();
          // var nv = n.val();
          var haserr = false;
          if (ev == "") {
            haserr = true;
          }
          if (!haserr) {
            try {
              $crisp.push(["set", "user:email", [ev]])
              $crisp.push(["set", "session:segments", [["newsletter"]]])
              $('#newsletter-confirm').show();
              $('#newsletter-form').hide();
            } catch(err) {
              console.error(err);
              haserr = true;
            }
          }
          if (haserr) {
            e.css("border", "1px solid red");
            return;
          } else {
            e.css("border", "1px solid grey");
          }
          // if (nv == "") {
          //   console.log("blank n")
          //   n.css("border", "1px solid red");
          //   return;
          // } else {
          //   n.css("border", "1px solid grey");
          // }


    }
      </script>

